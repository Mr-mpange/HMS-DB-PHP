# Production Environment Variables
# IMPORTANT: Update these values before deploying!

# Backend API URL (Update with your Railway/Render URL)
VITE_API_URL=https://your-backend-url.railway.app/api
VITE_SOCKET_URL=https://your-backend-url.railway.app

# ZenoPay Configuration (Production)
VITE_ZENOPAY_API_URL=https://zenoapi.com/api
VITE_ZENOPAY_API_KEY=your_production_zenopay_api_key
VITE_ZENOPAY_MERCHANT_ID=your_production_merchant_id
VITE_ZENOPAY_ENV=production

# Application
VITE_APP_URL=https://your-domain.com
VITE_APP_NAME=Hospital Management System
VITE_APP_VERSION=1.0.0

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
